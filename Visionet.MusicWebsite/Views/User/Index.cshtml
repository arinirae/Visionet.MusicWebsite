@model IEnumerable<Visionet.MusicWebsite.Models.User>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<table class="table">
    <tr>
        <th>
            Id User
        </th>
        <th>
            Name
        </th>
        
        <th></th>
        <th></th>

    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.IdUser)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Name)
            </td>
            <td>
                @Html.ActionLink("Details", "Details", new { id = item.IdUser })
            </td>
            <td>
                @Html.ActionLink("Add Friend", "AddFriend", new { id = item.IdUser })
            </td>
        </tr>
    }
</table>
